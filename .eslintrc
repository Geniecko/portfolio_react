{
   "extends": [
      "airbnb",
      "prettier",
      "react-app",
      "plugin:import/errors",
      "plugin:import/warnings"
   ],
   "parser": "babel-eslint",
   "env": {
      "jest": true
   },
   "globals": {
      "document":true
   },
   "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": ["node_modules", "src/"]
        }
      }
    },
   "rules": {
      "import/prefer-default-export": 0,
      "import/no-unresolved": 1,
      "react/jsx-filename-extension": [
         1, 
         { 
            "extensions": [
               ".js" 
            ] 
         }
      ]
    }   
}